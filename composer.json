{
    "name": "cloudycity/laravel-filesystem-obs",
    "description": "huawei cloud obs filesystem storage for laravel 5+",
    "keywords": [
        "huawei",
        "obs",
        "laravel",
        "filesystems",
        "storage"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "league/flysystem": "^1.1",
        "laravel/framework": "^5.2|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Obs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Obs\\ObsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
